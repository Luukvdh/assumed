{
  "name": "assumed",
  "version": "1.7.0",
  "description": "Typescript fast & easy asserts",
  "type": "module",
  "main": "./dist/assume.js",
  "exports": {
    ".": {
      "types": "./src/assume.ts",
      "import": "./dist/assume.js"
    },
    "./*": "./dist/*.js"
  },
  "files": [
    "dist/assume.js",
    "dist/assume.d.ts",
    "src/assume.ts",
    "src/assume.d.ts",
    "dist/primitiveprimer.global.min.mjs",
    "dist/assumed.global.min.mjs"
  ],
  "keywords": [
    "assert",
    "check",
    "javascript",
    "typescript"
  ],
  "homepage": "https://github.com/Luukvdh/assumed#readme",
  "bugs": {
    "url": "https://github.com/Luukvdh/assumed/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luukvdh/assumed.git"
  },
  "license": "ISC",
  "author": "Luuk van den Hoogen",
  "scripts": {
    "build": "tsc",
    "build:browser": "esbuild src/assume.ts --bundle --minify --format=iife --global-name=Assumed --platform=browser --outfile=dist/assumed.browser.min.mjs",
    "build:browserassert": "esbuild src/assertroute.ts --bundle --minify --format=iife --global-name=assertroute --platform=browser --outfile=dist/assertroute.browser.min.mjs",
    "clean": "rimraf dist public/js"
  },
  "devDependencies": {
    "esbuild": "^0.21.5"
  }
}
